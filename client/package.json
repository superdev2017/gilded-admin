{
  "name": "gilded",
  "version": "0.0.1",
  "license": "custom",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.config.json",
    "build": "ng build --env=prod --aot",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:free": "ngm build -p src/app/typescripts/free --clean && gulp npmFree && gulp startFree && gulp onlyFree",
    "build:pro": "ngm build -p src/app/typescripts/pro --clean && gulp only-pro && gulp startPro",
    "build:all": "npm run build:free && npm run build:pro",
    "build:watch": "ng build --watch --sourcemaps=false",
    "build:staging": "ng build --env=staging --aot",
    "aot:build": "ng build --prod --sm=false --aot=true --output-path=dist",
    "pre-commit": "ng lint",
    "format": "prettier --config ./.prettierrc --write \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\""
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0-beta.2",
    "@angular/animations": "^5.2.9",
    "@angular/common": "^5.2.9",
    "@angular/compiler": "^5.2.9",
    "@angular/core": "^5.2.9",
    "@angular/forms": "^5.2.9",
    "@angular/http": "^5.2.9",
    "@angular/platform-browser": "^5.2.9",
    "@angular/platform-browser-dynamic": "^5.2.9",
    "@angular/router": "^5.2.9",
    "@auth0/angular-jwt": "^1.1.0",
    "@ngrx/store": "^5.2.0",
    "@ngx-loading-bar/core": "^1.6.0",
    "@ngx-loading-bar/http": "^1.6.0",
    "@ngx-loading-bar/router": "^1.6.0",
    "@swimlane/ngx-datatable": "^11.3.2",
    "@types/lodash": "^4.14.108",
    "angular-request-network": "^1.0.5-rc",
    "angularfire2": "5.0.0-rc.6.0",
    "angularx-qrcode": "^1.1.7",
    "chart.js": "2.7.2",
    "classlist.js": "1.1.x",
    "core-js": "^2.5.4",
    "del": "3.0.x",
    "easy-pie-chart": "2.1.x",
    "elasticsearch": "^14.2.2",
    "firebase": "4.12.1",
    "font-awesome": "4.7.x",
    "hammerjs": "2.0.x",
    "jquery": "^3.3.1",
    "moment": "^2.22.1",
    "ngx-pagination": "^3.1.1",
    "raven-js": "^3.24.2",
    "rxjs": "^5.5.8",
    "screenfull": "3.3.x",
    "smoothscroll-polyfill": "0.4.3",
    "sweetalert2": "^7.19.2",
    "url-parse": "^1.4.0",
    "wallet-address-validator": "^0.1.7",
    "web-animations-js": "2.3.x",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.4",
    "@angular/compiler-cli": "^5.2.9",
    "@angular/language-service": "^5.2.9",
    "@types/elasticsearch": "^5.0.23",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^6.0.103",
    "codelyzer": "^4.3.0",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^1.0.0",
